<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<CHARTSTYLE name="blue" isDefault="false">

	<TEMPLATE>
	
		<generic>
			
			<CHART height="" seriesStacking="false" heightDimType="" width="" widthDimType="" isCockpitEngine="false" orientation="" style="fontFamily:Verdana;fontSize:12px;fontWeight:normal;backgroundColor:#E3E3E3;" styleName="blue">
	
				<TITLE style="align:center;fontFamily:Verdana;fontSize:16px;fontWeight:bold;color:#006699;" text=""/>
				
				<SUBTITLE style="align:center;color:#006699;fontFamily:Verdana;fontWeight:italic;fontSize:14px;" text=""/>
				
				<EMPTYMESSAGE style="align:left;color:#FF0000;fontFamily:Verdana;fontWeight:bold;fontSize:18px;" text=""/>
				
				<LEGEND layout="" position="top" show="false"
					style="align:;fontFamily:;fontSize:;fontWeight:;borderWidth:;color:;backgroundColor:transparent;">
					<TITLE style="align:;color:;fontFamily:;fontWeight:;fontSize:;" text=""/>
				</LEGEND>
				
				<COLORPALETTE>
					<COLOR id='1' order='1' name="7cb5ec" value="#7cb5ec"/>
					<COLOR id='2' order='2' name="434348" value="#434348"/>
					<COLOR id='3' order='3' name="90ed7d" value="#90ed7d"/>
					<COLOR id='4' order='4' name="f7a35c" value="#f7a35c"/>
					<COLOR id='5' order='5' name="8085e9" value="#8085e9"/>
					<COLOR id='6' order='6' name="f15c80" value="#f15c80"/>
					<COLOR id='7' order='7' name="e4d354" value="#e4d354"/>
					<COLOR id='8' order='8' name="2b908f" value="#2b908f"/>
					<COLOR id='9' order='9' name="f45b5b" value="#f45b5b"/>
					<COLOR id='10' order='10' name="91e8e1" value="#91e8e1"/>
				</COLORPALETTE>
				
				<AXES_LIST>
					
					<AXIS type='Serie' style="rotate:;align:;color:#006699;fontFamily:Verdana;fontWeight:normal;fontSize:12px;opposite:false;">
						<MAJORGRID interval="" style="typeline:;color:#D8D8D8;"/>
						<MINORGRID interval="" style="typeline:;color:#E0E0E0;"/>
						<TITLE style="align:;color:#006699;fontFamily:Verdana;fontWeight:bold;fontSize:14px;" text="Title for Y axis"/>
					</AXIS>
	   
					<AXIS type='Category' style="rotate:;align:;color:#006699;fontFamily:Verdana;fontWeight:normal;fontSize:12px;">
						<TITLE style="rotate:;align:;color:#006699;fontFamily:Verdana;fontWeight:bold;fontSize:14px;" text="Title for X axis"/>
					</AXIS>
				
				</AXES_LIST>
				
				<VALUES>					
					<SERIE color="" postfixChar="" precision="2" prefixChar="" showValue="true" >
						<TOOLTIP backgroundColor="" borderWidth="1" borderRadius="3" style="color:#006699;fontFamily:Verdana;fontWeight:;fontSize:10px;align:left;" />
					</SERIE>
				</VALUES>
			
			</CHART>
		
		</generic>
		
		<wordcloud>
			<CHART height="" width="" isCockpitEngine="false" orientation="" style="fontFamily:Impact;fontSize:12px;fontWeight:normal;backgroundColor:#E3E3E3;" type="WORDCLOUD" maxAngle='120' maxFontSize='60' minFontSize='5' wordLayout='horizontal' maxWords='60' preventOverlap='true' minAngle='45' sizeCriteria='serie' wordPadding='5'>
		    <WORDCLOUD_TOOLTIP backgroundColor="#E3E3E3" borderRadius="3" borderWidth="1" postfix="" precision="" prefix="" style="fontFamily:Verdana;fontSize:10px;fontWeight:normal;color:#006699;align:center;"/>
		    </CHART>
		</wordcloud>
	
		<bar>
			<CHART type="BAR"/>
		</bar>
		
		<line>
			<CHART type="LINE"/>
		</line>
	
		<pie>
			<CHART type="PIE"/>
		</pie>
		
		<treemap>
			<CHART type="TREEMAP"/>
		</treemap>
	
		<parallel>
			
			<CHART type="PARALLEL">
				
				<AXES_LIST style="axisColNamePadd:16;brushWidth:10;axisColor:#006699;brushColor:#006699;">
				
					<AXIS type='Serie' style="rotate:;align:;color:#006699;fontFamily:Verdana;fontWeight:normal;fontSize:12px;"/>
					
					<AXIS type='Category' style="color:#006699;fontFamily:Verdana;fontWeight:italic;fontSize:12px;"/>
				
				</AXES_LIST>
	
				<LIMIT style="maxNumberOfLines:20;orderTopMinBottomMax:bottom;serieFilterColumn:;"/>
					
				<PARALLEL_TOOLTIP style="fontFamily:Verdana;fontSize:18px;minWidth:10;maxWidth:250;minHeight:5;maxHeight:200;padding:1;border:1;borderRadius:1;"/>
					
				<LEGEND>
					<TITLE style="fontFamily:Verdana;fontSize:12px;fontWeight:bold;"/>
					<ELEMENT style="fontFamily:Verdana;fontSize:10px;fontWeight:normal;"/>
				</LEGEND>
			
			</CHART>
			
		</parallel>
		
		<heatmap>
			<CHART type="HEATMAP">
				<LEGEND symbolHeight='400' style="align:middle;"/>
				<TOOLTIP style="fontFamily:Verdana;fontSize:12px;color:#006699;fontWeight:bold;"/>
				<COLORPALETTE>
					<COLOR id='1' order='1' name="7cb5ec" value="#7cb5ec"/>
					<COLOR id='7' order='7' name="e4d354" value="#e4d354"/>
					<COLOR id='9' order='9' name="f45b5b" value="#f45b5b"/>
				</COLORPALETTE>
			</CHART>
		</heatmap>
		
		<radar>
			<CHART type="RADAR"/>
		</radar>
		
		<scatter>
			
			<CHART type="SCATTER" zoomType="xy">
			
				<AXES_LIST>
				
					<AXIS type='Serie' style="rotate:;align:;color:#006699;fontFamily:Verdana;fontWeight:normal;fontSize:12px;opposite:false;">
						<MAJORGRID interval="" style="typeline:;color:#D8D8D8;"/>
						<MINORGRID interval="" typeline="typeline:;color:#E0E0E0;"/>
						<TITLE style="align:;color:#006699;fontFamily:Verdana;fontWeight:bold;fontSize:14px;" text=""/>
					</AXIS>
					
					<AXIS type='Category' style="rotate:;align:;color:#006699;fontFamily:Verdana;fontWeight:normal;fontSize:12px;" startOnTick="false" showLastLabel="true" endOnTick="false">
						<TITLE style="align:;color:#006699;fontFamily:Verdana;fontWeight:bold;fontSize:14px;" text=""/>
					</AXIS>
				
				</AXES_LIST>
			
			</CHART>
	
		</scatter>
		
		<gauge>
		
			<CHART type="GAUGE">
		
				<AXES_LIST>
		
					<AXIS lineColor="" lineWidth="" minorTickColor="" minorTickLength="" minorTickPosition="inside" minorTickWidth="1"
			         offset="" minorTickInterval="" tickColor="#006699" tickLength="" type='Serie'
			         tickPixelInterval="" tickPosition="inside" tickWidth="2" style="rotate:;align:;color:#006699;fontFamily:Verdana;fontWeight:normal;fontSize:12px;opposite:false;">
						
						<TITLE style="align:;color:#006699;fontFamily:Verdana;fontWeight:bold;fontSize:14px;" text=""/>
				
						<LABELS distance="35" rotation="0"/>
				
						<PLOTBANDS>
							<PLOT color="" from="" to=""/>
						</PLOTBANDS>
					
					</AXIS>
			
					<AXIS type='Category' style="rotate:;align:;color:#006699;fontFamily:Verdana;fontWeight:bold;fontSize:14px;">
						<TITLE style="align:;color:;fontFamily:;fontWeight:;fontSize:;" text=""/>
					</AXIS>
			
				</AXES_LIST>
		
				<PANE endAngle="120" startAngle="-120"/>
		
				<VALUES>
	
					<SERIE animation="true" color="" pointInterval="" postfixChar="" precision="" prefixChar="" showInLegend="true"
		                   showValue="true">
						<TOOLTIP backgroundColor="#EEDDAA" style="color:#006699;fontFamily:Verdana;fontWeight:bold;fontSize:14px;align:left"/>
					</SERIE>
				
				</VALUES>
			
			</CHART>
			
		</gauge>
		
		<sunburst>
			<CHART type="SUNBURST" opacMouseOver="10">
				<TOOLBAR  style="position:bottom;spacing:5;tail:10;percFontColor:#006699;fontFamily:Verdana;fontWeight:normal;fontSize:14px;"/>
				<TIP style="fontFamily:Verdana;fontWeight:bold;fontSize:14px;color:#006699;width:200;" text=""/>
			</CHART>
		</sunburst>
		
		<chord>
			<CHART type="CHORD">
				<TOOLTIP style="fontFamily:Verdana;fontSize:16px;border:1;borderRadius:1;"/>
			</CHART>
		</chord>
	
	</TEMPLATE>
</CHARTSTYLE>